HIGH:

#include<stdio.h>
int main(){
    for(int i=1;i<=5;i++){
        for (int j=1;j<=9;j++){
            if (     i==j||j==5-i+1   ){
                printf("%d",j);
            }
                else{
                    printf("  ");
                }
            }
            printf("\n");
        }
        return 0;
    }
    =====================================================================================================================================================================================
 MEDIUM:   
 
#include<stdio.h>
int main(){
    int num,reverse=0,remainnder;
    scanf("%d",&num);
    while(num!=0){
        remainnder=num%10;
        reverse=reverse*10+remainnder;
        num/=10;
    }
    printf("%d",reverse);
    
    return 0;
}
========================================================================================================================================================================================================
LOW:
#include<stdio.h>
int main(){
    int n , i,odd=1,sum=0;
    scanf("%d",&n);
    for(i=1;i<=n;i++){
        printf("%d",odd);
        sum+=odd;
        odd+=2;
    }
    printf("\nsum of odd natural number up to %d term : %d ",n , sum);
    return 0;
}
